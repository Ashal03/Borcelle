                    for (var k in productSpecList[j].ProductSpecData) {
                        if (productSpecList[j].ProductSpecData[k].SpecId === productSpecItem[i].SpecId) {
                            //console.log(productSpecList[j].ProductSpecData[k].Description === '');
                            if (productSpecList[j].ProductSpecData[k].Description === '') {
                                countNoDescItem = countNoDescItem + 1;
                            }
                        }
                    }
                }
                //console.log(countNoDescItem);
                if (countNoDescItem < totalModelCount) {
                    productSpecItemClone.push(productSpecItem[i]);
                }
            }
            //console.log(productSpecItemClone);
            return productSpecItemClone;
        }


        var callJqueryForIe8SpecList = function(SpecJson) {
            var totalSpec = CheckShowSpecTitle(SpecJson.ProductSpecItem, SpecJson.ProductSpecList);
            var allChildProducts = SpecJson.ProductSpecList;

            for (var j in totalSpec) {
                var specValueDiv = $("<div></div>").addClass("specText").append(totalSpec[j].Name);
                var specTitleDiv = $("<div></div>").addClass("specRow row_" + j).append(specValueDiv);
                $(".all-Spec").append(specTitleDiv);
            }
            for (var i in allChildProducts) {
                if (i < allChildProducts.length) {
                    var childModelTemp = allChildProducts[i].ProductSpecData;
                    
                    var childModel = CheckChildProductSpecEmptyDesc(totalSpec, childModelTemp);
                    var model = $("<div></div>").addClass("childModel");
                    if (allChildProducts.length > 1) {
                        var modelName = $("<div></div>").addClass("name productName_" + i).append(allChildProducts[i].Name);
                        model.append(modelName);
                    }
                    for (var k in childModel) {
                        var specText = $("<div></div>").addClass("specText");
                        var specTitle = $("<div></div>").addClass("specTitle").append(childModel[k].Name);
                        var specDec = $("<div></div>").addClass("specDesc").append(childModel[k].Description);
                        specText.append(specTitle);
                        specText.append(specDec);
                        var modelSpecRow = $("<div></div>").addClass("specRow row_" + k);
                        if (SpecJson.BackgroundType === 0) {
                            modelSpecRow.addClass("white");
                        }
                        modelSpecRow.append(specText);
                        model.append(modelSpecRow);
                    }
                    $(".owl-area").append(model);
                }
            }
            if (window.gbtGlobal.Razor.IsCN) {
                setTimeout(rePlaceCnAdKeywordByobj($('body')), 2000);
            }
        }

        var callVueForSpecList = function(SpecJson) {
            new Vue({
                el: '#vue-list-spec',
                data: {
                    allData: SpecJson,
                    Products: SpecJson.ProductSpecList,
                    BackgroundType: SpecJson.BackgroundType
                },
                methods: {
                },
                computed: {
                    isWhiteStyle: function() {
                        return {
                            white: this.BackgroundType === 0
                        }
                    },
                    getSpec: function() {
                        var result = [];
                        var totalSpec = SpecJson.ProductSpecItem;
                        var allChildProducts = SpecJson.ProductSpecList;
                        for (var i in allChildProducts) {
                            
                            CheckChildProductSpec(totalSpec, allChildProducts[i].ProductSpecData);
                            result.push(allChildProducts[i]);
                        }
                        return result;
                    }
                }
            });
        }
        var setModelNameVersionStyle = function() {
            var title = $('.pageTitle');
            if (title) {
                var removeSpaceStr = title.text().trim().replace(/\r\n|\n/g, "");
                if (typeof (removeSpaceStr) !== 'undefined') {
                    title.text(removeSpaceStr);
                }
                if (title.text().indexOf('(rev.') !== -1) {
                    if (title.text().indexOf(')') !== -1) {
                        var replaceText = title.text().replace("(rev.", "<div id='model_version'>(rev.").replace(")", ")</div>");
                        if (typeof (replaceText) !== 'undefined') {
                            title.html(replaceText);
                        }
                    }
                }
            }
        };

        var setCloseRelatedSolutions = function() {
            $(".CloseIcon").off('click').click(function(e) {
                $(this).parents(".RelatedSolutions").fadeOut(600);
                e.stopPropagation();
            });
        }

        var setOwlCarouselAnimationSliderByCreateDom = function(parentDiv, parentChildInfo) {
            var mobileArea = $(".Content-Mobile-SliderItemList");
            mobileArea.find(".CloseButton, .cover").off('click').click(function() {
                SetMobileBottomSliderSlideUp(mobileArea);
            });
            $(".RelatedSolutions").off('click').click(function() {
                SetMobileBottomSliderSlideBeforeCheckOwlCarousel(mobileArea);
            });
        }

        
        var setOwlCarouselAnimationSliderInit = function(parentDiv) {
            var owlDiv = parentDiv.find(".SliderItems");
            if (owlDiv.length) {
                owlDiv.owlCarousel({
                    lazyLoad: true,
                    loop: false,
                    rtl: window.gbtGlobal.Razor.IsRTL,
                    dots: true,
                    onInitialized: SetMobileBottomSliderSlide(parentDiv),
                    checkVisible: false,
                    responsive: {
                        0: {
                            items: 1,
                            margin: 10, //避免圖片陰影下則Solution 的縮圖部分會出現在上則的畫面上
                        },
                        640: {
                            dotsEach: 1,
                            items: 2,
                        }
                    }
                });
            }
        }

        
        var SetMobileBottomSliderSlideBeforeCheckOwlCarousel = function(parentDiv) {
            if (parentDiv.find(".SliderItems ").attr("class").indexOf("owl-loaded") < 0) {
                setOwlCarouselAnimationSliderInit(parentDiv);
            } else {
                SetMobileBottomSliderSlide(parentDiv);
            }
        }

        
        var SetMobileBottomSliderSlide = function(parentDiv) {
            parentDiv.find(".cover").removeClass("hide");
            parentDiv.find(".Content-Mobile-SliderItems").slideDown("fast",
                function() {
                    if (CheckWebsiteUnder960()) {
                        $("body").addClass("lockScroll");
                        setHighByParentChildDiv("SliderItem", "SliderItemName");
                    }
                });
        }

        
        var SetMobileBottomSliderSlideUp = function(mobileArea) {
            mobileArea.find(".Content-Mobile-SliderItems").slideUp("fast",
                function() {
                    $("body").removeClass("lockScroll");
                    mobileArea.find(".cover").addClass("hide");
                });
        }

        var setHighByParentChildDiv = function(parent, child) {
            var childDiv = $("." + parent).find("." + child);
            if (childDiv.length) {
                childDiv.each(function() {
                    $(this).css("height", "auto");
                });
                var maxHigh = getListDivMaxHigh(parent, child);
                if (parent === "childItem-Product" && child === "sectionChild-img") {
                    //console.log(maxHigh);
                }
                childDiv.each(function() {
                    $(this).height(maxHigh);
                });
            }
        }

        var getListDivMaxHigh = function(parentDiv, childDiv) {
            var maxImgHeight = Math.max.apply(null,
                $("." + parentDiv).find("." + childDiv).map(function() {
                    return $(this).height();
                }).get());
            return maxImgHeight;
        }

        var DownloadLinkRedirect = function() {
            var nowHash = location.hash;
            var nowUrl = location.href.toLowerCase();
            var link = "";
            switch (nowHash) {
            case "#support-dl":
                if (nowUrl.indexOf("/support") < 0) {
                    link = $("li[data-type='support']").find("a").attr("href");
                }
                break;
            }
            return link;
        }
