        function getChildPidByHash() {
            var hashLength = window.hashArray.length;
            var childPid = 0;
            if (typeof (window.hashArray[hashLength - 1]) !== 'undefined') {
                var subHashType = window.hashArray[hashLength - 1];
                if (typeof (subHashType) !== 'undefined' && subHashType !== '') {
                    if (subHashType.indexOf('pid') !== -1) {
                        childPid = subHashType.replace('pid', '');
                        childPid = parseInt(childPid);
                    }
                }
            }
            return childPid;
        }

